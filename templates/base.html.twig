<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %} La FleurS{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <!-- bootstrap -->
        <link rel="stylesheet" href="{{ asset ('Styles/css/bootstrap-4.5.3-dist/css/bootstrap.min.css') }}">
        <!-- FontAwesome CSS -->
        <link rel="stylesheet" href="{{ asset ('Styles/css/fontawesome-free-515.1-web/css/all.min.css') }}">




    </head>
    <body style="padding-top: 56px">
        
        <!-- Navigation -->
        {{ include("menu.html.twig")}}


        <!-- Page Content -->
        <div class="container" style="margin:auto">
            {% block body %} 

            {% endblock %}
        </div>
        <!-- /.container -->

        <!-- Footer -->
        <footer class="py-5 bg-dark" style="margin-top:40px">
            <!-- container -->
            <div class="container">
                <p class="m-0 text-center text-white">Copyright &copy; La Fleur 2020</p>
            </div>
            <!-- /.container -->
        </footer>

        <!-- Bootstrap core JavaScript -->
        <script src="Public/Styles/vendor/jquery/jquery.min.js"></script>
        <script src="Public/Styles/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        

          {% block javascripts %}
         <!-- Optional JavaScript -->
        <!-- jQuery first, then Popper.js, then Bootstrap JS -->
        <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="{{ asset('css/bootstrap-4.5.3-dist/js/bootstrap.min.js') }}"></script>
         {% endblock %}
    </body>
</html>
